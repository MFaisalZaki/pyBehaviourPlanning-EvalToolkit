    "version": "0.2.0",
    "configurations": [
        {
            "name": "Generate solve experiment commands",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/exp-runner/main.py",
            "console": "integratedTerminal",
            "args": ["generate", "--partition", "sturm-part",
            "--exp-details-dir", "${workspaceFolder}/exps/utility-diverse-planning",
            "--planning-tasks-dir", "${workspaceFolder}/external-pkgs/classical-domains",
            "--sandbox-dir", "${workspaceFolder}/sandbox-diverse-utility-planning"
            ],
            "justMyCode": false
        },
        {
            "name": "Generate score experiment commands",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/exp-runner/main.py",
            "console": "integratedTerminal",
            "args": ["generate", 
            "--exp-details-dir", "${workspaceFolder}/exps/oversubscription-diverse-planning/0.5", 
            "--sandbox-dir", "${workspaceFolder}/sandbox-oversubscription-behaviour-count-exp/0.5", 
            "--planning-tasks-dir", "${workspaceFolder}/external-pkgs/classical-domains",
            "--partition", "sturm-part",
            "--for-score-exp",
            "--score-for-k", "5", "10", "100", "1000"],
            "justMyCode": false
        },
        {
            "name": "Run solve experiment command",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/exp-runner/main.py",
            "console": "integratedTerminal",
            "args": ["solve", 
            "--experiment-file", 
            // "${workspaceFolder}/sandbox-oversubscription-behaviour-count-exp/0.5/experiment-tasks/2006-rovers-3-1.0-1000-fbi-utility-set.json"
            "${workspaceFolder}/sandbox-classical-behaviour-count-exp/experiment-tasks/2006-rovers-3-2.0-1000-fbi-seq.json"
            ],
            "justMyCode": false
        },
        {
            "name": "Run score experiment command",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/exp-runner/main.py",
            "console": "integratedTerminal",
            "args": ["score", 
            //"--experiment-file", "${workspaceFolder}/sandbox-classical-behaviour-count-exp/score-exp-files/1998-gripper-1-1.0-5-fi-none-bspace.json",
            //"--k", "5"
            
            "--experiment-file", "${workspaceFolder}/sandbox-oversubscription-behaviour-count-exp/0.5/dump-results/2006-rovers-1-1.0-1000-symk-util-value.json",
            "--k", "10"

            ],
            "justMyCode": false
        },
        {
            "name": "Analyze experiment results",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/exp-analyzer/main.py",
            "console": "integratedTerminal",
            "args": [
                "analyze", 
                "--compute-behaviour-count",
                "--compute-coverage",
                
                "--planner-results-dir", "${workspaceFolder}/sandbox-classical-behaviour-count-exp/score-dump-results",
                "--output-dir", "${workspaceFolder}/sandbox-classical-behaviour-count-exp/analysis-score-dump-results"
                // "--planner-results-dir", "${workspaceFolder}/sandbox-classical-behaviour-count-exp/no-validation",
                // "--output-dir", "${workspaceFolder}/sandbox-classical-behaviour-count-exp/try-catch"
            ],
            "justMyCode": false
        },
        {
            "name": "Summarise experiment errors",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/exp-analyzer/main.py",
            "console": "integratedTerminal",
            "args": [
                "summarise-errors",
                "--error-files-dir", "${workspaceFolder}/sandbox-classical-behaviour-count-exp/impl6-solve-run-dir",
                "--output-dir", "${workspaceFolder}/sandbox-classical-behaviour-count-exp/impl6-solve-run-dir",
            ],
            "justMyCode": false
        },
        {
            "name": "replace domain/problem files",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/exp-modifiers/main.py",
            "console": "integratedTerminal",
            "args": ["replace",
            "--problem-files",
            "--tag", "/home/ma342/oversubscription-all-run-1",
            "--value", "/Users/mustafafaisal/Developer/dev-pybehaviour-planning-evaltoolkit",
            "--dir", "${workspaceFolder}/sandbox-oversubscription-behaviour-count-exp/1.0/dump-results",
            ],
            "justMyCode": false
        },
        {
            "name": "create a copy for symk",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/exp-modifiers/main.py",
            "console": "integratedTerminal",
            "args": ["update-symk",
            "--cost-bound", "0.5",
            "--dir", "${workspaceFolder}/sandbox-oversubscription-behaviour-count-exp/0.5/dump-results",
            "--dump-dir", "${workspaceFolder}/sandbox-oversubscription-behaviour-count-exp/0.5/updated-symk-dump-results"
            ],
            "justMyCode": false
        }
    ]
}
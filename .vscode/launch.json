    "version": "0.2.0",
    "configurations": [
        {
            "name": "Generate solve experiment commands",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/exp-runner/main.py",
            "console": "integratedTerminal",
            "args": ["generate", "--partition", "sturm-part",
            "--exp-details-dir", "${workspaceFolder}/exps/utility-diverse-planning",
            "--planning-tasks-dir", "${workspaceFolder}/external-pkgs/classical-domains",
            "--sandbox-dir", "${workspaceFolder}/sandbox-diverse-utility-planning"


            // "--exp-details-dir", "${workspaceFolder}/exps/exp-fbi-different-encodings",
            // "--planning-tasks-dir", "${workspaceFolder}/external-pkgs/classical-domains",
            // "--sandbox-dir", "${workspaceFolder}/sandbox-classical-fbi-different-encodings"

            // "--exp-details-dir", "${workspaceFolder}/exps/numeric-behaviour-count/fbi",
            // "--planning-tasks-dir", "${workspaceFolder}/external-pkgs/numeric-domains",
            // "--sandbox-dir", "${workspaceFolder}/sandbox-numeric"

            // "--exp-details-dir", "${workspaceFolder}/exps/exp-fbi-behaviour-count",
            // "--planning-tasks-dir", "${workspaceFolder}/external-pkgs/classical-domains",
            // "--sandbox-dir", "${workspaceFolder}/sandbox-classical"
            ],
            "justMyCode": false
        },
        {
            "name": "Generate score experiment commands",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/exp-runner/main.py",
            "console": "integratedTerminal",
            "args": ["generate", 
            "--exp-details-dir", "${workspaceFolder}/exp-baseline-fbi", 
            "--sandbox-dir", "${workspaceFolder}/sandbox", 
            "--planning-tasks-dir", "${workspaceFolder}/external-pkgs/classical-domains",
            "--partition", "sturm-part",
            "--for-score-exp",
            "--score-for-k", "5", "10", "100", "1000"],
            "justMyCode": false
        },
        {
            "name": "Run solve experiment command",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/exp-runner/main.py",
            "console": "integratedTerminal",
            "args": ["solve", 
            "--experiment-file", 
            // "${workspaceFolder}/sandbox-numeric/experiment-tasks/None-rover-1-1.0-1000-fbi-new-implementation.json"

            // "${workspaceFolder}/sandbox-numeric/experiment-tasks/None-rover-1-1.0-1000-fbi.json"
            // "${workspaceFolder}/sandbox-numeric/experiment-tasks/None-ext-plant-watering-4-1.0-1000-fbi.json"
            // "${workspaceFolder}/sandbox-numeric/experiment-tasks/None-satellite-1-1.0-1000-fbi.json"

            // "${workspaceFolder}/sandbox-classical-behaviour-count-exp/fbi/experiment-tasks/2006-rovers-3-2.0-1000-fbi.json"
            // "${workspaceFolder}/sandbox-classical-behaviour-count-exp/symk/experiment-tasks/2006-rovers-3-2.0-1000-symk.json"
            // "${workspaceFolder}/sandbox-classical-behaviour-count-exp/fi-bspace/experiment-tasks/2006-rovers-2-1.0-100-fi-bspace.json"            


            // "${workspaceFolder}/sandbox-fbi-different-encodings/experiment-tasks/2006-rovers-1-2.0-5-fbi-qfuf.json"
            // "${workspaceFolder}/sandbox-fbi-different-encodings/experiment-tasks/2006-rovers-1-2.0-5-fbi-seq.json"
            // "${workspaceFolder}/sandbox-fbi-different-encodings/experiment-tasks/2006-rovers-1-2.0-5-fbi-r2e.json"

            "${workspaceFolder}/sandbox-numeric-behaviour-count-exp/sandbox-fbi-behaviour-count/experiment-tasks/None-depots-1-1.0-1000-fbi-seq.json"

            // "${workspaceFolder}/sandbox-diverse-utility-planning/experiment-tasks/2006-rovers-3-1.0-1000-fbi.json"
            // "${workspaceFolder}/sandbox-diverse-utility-planning/experiment-tasks/2006-rovers-3-1.0-1000-symk.json"
            ],
            "justMyCode": false
        },
        {
            "name": "Run score experiment command",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/exp-runner/main.py",
            "console": "integratedTerminal",
            "args": ["score", 
            "--experiment-file", "${workspaceFolder}/sandbox-fi-behaviour-count/dump-results/1998-gripper-1-1.0-100-fi.json",
            "--k", "10"
            ],
            "justMyCode": false
        },
        {
            "name": "Analyze experiment results",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/exp-analyzer/main.py",
            "console": "integratedTerminal",
            "args": [
                "analyze", 
                "--compute-behaviour-count",
                "--compute-coverage",

                "--planner-results-dir", "${workspaceFolder}/sandbox-classical-behaviour-count-exp/dump-results",
                "--output-dir", "${workspaceFolder}/sandbox-classical-behaviour-count-exp/results"
            ],
            "justMyCode": false
        },
        {
            "name": "Summarise experiment errors",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/summaries-errors.py",
            "console": "integratedTerminal",
            "args": [
            "--error-dir", "${workspaceFolder}/sandbox/solve-run-errors",
            "--bspace-results-dir", "${workspaceFolder}/sandbox/score-results",
            ],
            "justMyCode": false
        },
        {
            "name": "Summarise experiment run results",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/summaries-results.py",
            "console": "integratedTerminal",
            "args": [
            "--results-dir", "${workspaceFolder}/sandbox-fbi-baseline/dump-results"
            ],
            "justMyCode": false
        },
        {
            "name": "Analyze experiment run results",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/analyze-results.py",
            "console": "integratedTerminal",
            "args": [
            "--scores-dir", "${workspaceFolder}/sandbox/score-results"
            ],
            "justMyCode": false
        }
    ]
}